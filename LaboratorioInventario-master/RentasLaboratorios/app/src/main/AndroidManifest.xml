<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="com.example.josepablomontoya.rentaslaboratorios">

    <uses-permission android:name="android.permission.INTERNET" />

    <application
        android:allowBackup="true"
        android:icon="@mipmap/ic_launcher"
        android:label="@string/app_name"
        android:roundIcon="@mipmap/ic_launcher_round"
        android:supportsRtl="true"
        android:theme="@style/AppTheme">
        <activity
            android:name=".MainActivity"
            android:label="@string/app_name"
            android:theme="@style/AppTheme.NoActionBar">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
        <activity
            android:name=".Rentar"
            android:label="@string/title_activity_rentar"
            android:theme="@style/AppTheme.NoActionBar" />
        <activity
            android:name=".Consultar"
            android:label="@string/title_activity_consultar"
            android:theme="@style/AppTheme.NoActionBar" />
        <activity
            android:name=".NuevoUsuario"
            android:label="@string/title_activity_nuevo_usuario"
            android:theme="@style/AppTheme.NoActionBar" />
        <activity
            android:name=".Devolver"
            android:label="@string/title_activity_devolver"
            android:theme="@style/AppTheme.NoActionBar" />
        <activity
            android:name=".DevolverProductos"
            android:label="@string/title_activity_devolver_productos"
            android:theme="@style/AppTheme.NoActionBar" />
        <activity
            android:name=".AgregarProducto"
            android:label="@string/title_activity_agregar_producto"
            android:theme="@style/AppTheme.NoActionBar" />
        <activity
            android:name=".GetProductos"
            android:label="@string/title_activity_get_productos"
            android:theme="@style/AppTheme.NoActionBar"></activity>
    </application>

</manifest>